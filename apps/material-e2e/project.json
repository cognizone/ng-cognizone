{
  "name": "material-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/material-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/material-e2e/cypress.json",
        "devServerTarget": "material:storybook",
        "testingType": "e2e",
        "tsConfig": "apps/material-e2e/tsconfig.json"
      },
      "configurations": {
        "ci": {
          "devServerTarget": "material:storybook:ci"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/material-e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["material"]
}
