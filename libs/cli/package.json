{
  "name": "@cognizone/cli",
  "version": "3.11.0",
  "bin": {
    "cz-cli": "./bin/run"
  },
  "homepage": "https://github.com/cognizone/ng-cognizone",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cognizone/ng-cognizone.git",
    "directory": "libs/cli"
  },
  "files": [
    "/assets",
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.19.1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.6",
    "@types/lodash": "^4.14.188",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "cosmiconfig": "^7.0.0",
    "execa": "^4.0.0",
    "handlebars": "^4.7.2",
    "immer": "^9.0.1",
    "jsonc-parser": "^3.0.0",
    "listr": "^0.14.3",
    "lodash": "^4.17.21",
    "prompts": "^2.3.0",
    "rxjs": "^7.0.0",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.8",
    "@types/chai": "^4",
    "@types/handlebars": "^4.1.0",
    "@types/listr": "^0.14.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.3",
    "@types/prompts": "^2.0.3",
    "@types/shelljs": "^0.8.6",
    "chai": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-oclif": "^4",
    "eslint": "^7.32.0",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.8.4"
  },
  "optionalDependencies": {
    "@cognizone/application-profile": ">=3.0.0",
    "@cognizone/model-utils": ">=3.0.0"
  },
  "oclif": {
    "bin": "cz-cli",
    "dirname": "cz-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.spec.ts\""
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/WilliamChelman/hello-world/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "volta": {
    "node": "18.15.0",
    "npm": "9.6.1"
  },
  "contributors": [
    {
      "name": "William Chelman",
      "email": "william.chelman@cogni.zone"
    },
    {
      "name": "Farah El Agha",
      "email": "farah.agha111@gmail.com"
    }
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}