{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "docs-app",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "ng-core": {
      "projectType": "library",
      "root": "libs/ng-core",
      "sourceRoot": "libs/ng-core/src",
      "prefix": "cz",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/ng-core/tsconfig.lib.json",
            "project": "libs/ng-core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ng-core/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ng-core/src/**/*.ts", "libs/ng-core/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ng-core"],
          "options": {
            "jestConfig": "libs/ng-core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "model-utils": {
      "projectType": "library",
      "root": "libs/model-utils",
      "sourceRoot": "libs/model-utils/src",
      "prefix": "cz",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/model-utils/tsconfig.lib.json",
            "project": "libs/model-utils/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/model-utils/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/model-utils/src/**/*.ts", "libs/model-utils/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/model-utils"],
          "options": {
            "jestConfig": "libs/model-utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "workspace": {
      "architect": {
        "version": {
          "builder": "@jscutlery/semver:version",
          "options": {
            "syncVersions": true
          }
        }
      },
      "root": "."
    },
    "application-profile": {
      "projectType": "library",
      "root": "libs/application-profile",
      "sourceRoot": "libs/application-profile/src",
      "prefix": "cz",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/application-profile/tsconfig.lib.json",
            "project": "libs/application-profile/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/application-profile/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/application-profile/src/**/*.ts", "libs/application-profile/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/application-profile"],
          "options": {
            "jestConfig": "libs/application-profile/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "devtools": {
      "projectType": "library",
      "root": "libs/devtools",
      "sourceRoot": "libs/devtools/src",
      "prefix": "cz",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/devtools/tsconfig.lib.json",
            "project": "libs/devtools/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/devtools/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/devtools/src/**/*.ts", "libs/devtools/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/devtools"],
          "options": {
            "jestConfig": "libs/devtools/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ng-application-profile": {
      "projectType": "library",
      "root": "libs/ng-application-profile",
      "sourceRoot": "libs/ng-application-profile/src",
      "prefix": "cz",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/ng-application-profile/tsconfig.lib.json",
            "project": "libs/ng-application-profile/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ng-application-profile/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ng-application-profile/src/**/*.ts", "libs/ng-application-profile/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ng-application-profile"],
          "options": {
            "jestConfig": "libs/ng-application-profile/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "elastic": {
      "projectType": "library",
      "root": "libs/elastic",
      "sourceRoot": "libs/elastic/src",
      "prefix": "cz",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/elastic/tsconfig.lib.json",
            "project": "libs/elastic/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/elastic/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/elastic/src/**/*.ts", "libs/elastic/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/elastic"],
          "options": {
            "jestConfig": "libs/elastic/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "transloco-langstring": {
      "projectType": "library",
      "root": "libs/transloco-langstring",
      "sourceRoot": "libs/transloco-langstring/src",
      "prefix": "cz",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/transloco-langstring/tsconfig.lib.json",
            "project": "libs/transloco-langstring/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/transloco-langstring/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/transloco-langstring/src/**/*.ts", "libs/transloco-langstring/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/transloco-langstring"],
          "options": {
            "jestConfig": "libs/transloco-langstring/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ng-yasgui": {
      "projectType": "library",
      "root": "libs/ng-yasgui",
      "sourceRoot": "libs/ng-yasgui/src",
      "prefix": "cz",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/ng-yasgui/tsconfig.lib.json",
            "project": "libs/ng-yasgui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ng-yasgui/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ng-yasgui/src/**/*.ts", "libs/ng-yasgui/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ng-yasgui"],
          "options": {
            "jestConfig": "libs/ng-yasgui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "docs-app": {
      "projectType": "application",
      "root": "apps/docs-app",
      "sourceRoot": "apps/docs-app/src",
      "prefix": "cz",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "docs",
            "index": "apps/docs-app/src/index.html",
            "main": "apps/docs-app/src/main.ts",
            "polyfills": "apps/docs-app/src/polyfills.ts",
            "tsConfig": "apps/docs-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/docs-app/src/favicon.ico",
              "apps/docs-app/src/assets",
              {
                "glob": "**/*",
                "input": "apps/docs-app/src/styles",
                "output": "./assets/src/styles/"
              },
              {
                "glob": "**/*",
                "input": "apps/docs-app/src/app",
                "output": "./assets/src/app/"
              },
              {
                "glob": "**/*",
                "input": "libs",
                "output": "./assets/libs/"
              },
              {
                "glob": "**/*",
                "input": "dist/docs",
                "output": "./assets/docs/"
              },
              {
                "glob": "**/*",
                "input": "dist/cognizone/ng-yasgui/src/assets",
                "output": "./assets/ng-yasgui/"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["apps/docs-app/src/styles"]
            },
            "styles": ["apps/docs-app/src/styles/main.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/docs-app/src/environments/environment.ts",
                  "with": "apps/docs-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "docs-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "docs-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "docs-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/docs-app/src/**/*.ts", "apps/docs-app/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/docs-app"],
          "options": {
            "jestConfig": "apps/docs-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "docs-app-e2e": {
      "root": "apps/docs-app-e2e",
      "sourceRoot": "apps/docs-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/docs-app-e2e/cypress.json",
            "tsConfig": "apps/docs-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "docs-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "docs-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/docs-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "permissions": {
      "projectType": "library",
      "root": "libs/permissions",
      "sourceRoot": "libs/permissions/src",
      "prefix": "cz",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/permissions/tsconfig.lib.json",
            "project": "libs/permissions/ng-package.json"
          },
          "configurations": {
            "production": {
